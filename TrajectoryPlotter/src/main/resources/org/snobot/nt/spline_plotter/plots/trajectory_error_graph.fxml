<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="mPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.snobot.nt.spline_plotter.plots.TrajectoryErrorGraphController">
  <center>
    <LineChart xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
               fx:id="mChart">
      <xAxis>
        <NumberAxis side="BOTTOM"  fx:id="mXAxis"/>
      </xAxis>
      <yAxis>
        <NumberAxis side="LEFT"  fx:id="mYAxis"/>
      </yAxis>
    </LineChart>
  </center>
</BorderPane>